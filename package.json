{
  "name": "express-mongodb-api",
  "version": "1.0.0",
  "description": "API service with Express, MongoDB, JWT and Swagger",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "init": "node init-remote-mongo.js",
    "test": "jest --forceExit --detectOpenHandles",
    "test:watch": "jest --watch --forceExit",
    "test:coverage": "jest --coverage --forceExit",
    "test:auth": "jest --forceExit --detectOpenHandles test/routes/auth.test.js",
    "test:urls": "jest --forceExit --detectOpenHandles test/routes/urls.test.js",
    "test:hamibot": "jest --forceExit --detectOpenHandles test/routes/hamibot.test.js",
    "test:mobiles": "jest --forceExit --detectOpenHandles test/routes/mobiles.test.js",
    "test:users": "jest --forceExit --detectOpenHandles test/routes/users.test.js"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mongoose": "^7.5.0",
    "node-cache": "^5.1.2",
    "node-schedule": "^2.1.1",
    "p-limit": "^3.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.4"
  }
}
